BasicAttack = 
{
	name = "Basic Attack",
	description = "Attacks the target Card, dealing damage equal to your Attack Power.",
	targets = 1,
	
	effect = (target) ->
		{
			if (target == null || this.parent.parent == target.parent)
				return;
			
			target.setHealth(target.health - this.parent.attack, this);
		}
}